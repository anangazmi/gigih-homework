{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier",
        "import"
    ],
    "rules" : {
        "import/prefer-default-export": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "prettier/prettier": "warn",
        "no-console":"warn",
        "no-unused-vars":"warn"
    }
}
